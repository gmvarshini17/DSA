def closest_element(arr, k):
    closest = arr[0]
    min_diff = abs(arr[0] - k)
   
    for num in arr:
        diff = abs(num - k)
        if diff < min_diff or (diff == min_diff and num < closest):
            closest = num
            min_diff = diff
   
    return closest

n = int(input())  
arr = list(map(int, input().split()))
k = int(input())  


print(closest_element(arr, k))
